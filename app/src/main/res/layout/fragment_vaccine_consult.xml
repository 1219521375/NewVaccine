<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.pokestar.vaccineremind.ui.fragment.VaccineConsultFragment">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/vac_consult"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="查询"/>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="假疫苗流向"/>
        <Button
            android:id="@+id/vac_map_cc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="长春长生制药"/>
        <Button
            android:id="@+id/vac_map_wh"
            android:text="武汉生物制药"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />



    </LinearLayout>


</LinearLayout>
